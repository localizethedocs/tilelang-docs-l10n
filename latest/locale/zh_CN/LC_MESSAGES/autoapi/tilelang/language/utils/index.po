msgid "linear_index(i) -> i"
msgstr ""

#: ../../../autoapi/tilelang/language/utils/index.rst:67
msgid ""
"linear_index(i, j) -> i * j_stride + j  (requires j_stride provided as "
"stride when needed)"
msgstr ""

#: ../../../autoapi/tilelang/language/utils/index.rst:68
msgid "linear_index(i, j, stride_j) -> i * stride_j + j"
msgstr ""

#: ../../../autoapi/tilelang/language/utils/index.rst:69
msgid ""
"linear_index(i, j, k, stride_j, stride_k) -> i*stride_j*stride_k + "
"j*stride_k + k"
msgstr ""

#: ../../../autoapi/tilelang/language/utils/index.rst:70
msgid ""
"linear_index(i, tx, v, threads, local_size) -> i*threads*local_size + "
"tx*local_size + v"
msgstr ""

